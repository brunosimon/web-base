{% extends ajax ? "desktop/default/empty.html.twig" : "desktop/default/layout.html.twig" %}

{% block content %}

<div class="center">
	<h1>{{ content.title }}</h1>

	<ul>
	{% for work in content.works %}
		<li>
			<a href="{{ root_web ~ lang ~ work.path }}">
				{{ work.title }}
			</a>
		</li>
	{% endfor %}
	</ul>
</div>

{% endblock %}